{
  "mcpServers": {
    "database": {
      "command": "npx",
      "args": ["-y", "@benborla/mcp-server-mysql"],
      "env": {
        "MYSQL_URL": "mysql://root:@127.0.0.1:3306/staging"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "."]
    },
    "logs": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "./storage/logs"]
    }
  }
}
